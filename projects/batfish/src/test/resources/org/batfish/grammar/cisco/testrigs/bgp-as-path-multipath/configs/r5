! This node receives 1.0.0.0/32 from r2 and forwards it to r7
hostname r5
!
interface Loopback0
 ip address 5.0.0.0/32
!
interface Ethernet2
 ip address 10.25.0.5/24
 no shutdown
!
interface Ethernet7
 ip address 10.57.0.5/24
 no shutdown
!
route-map deny-all deny 100
!
router bgp 5
 router-id 5.0.0.0
 !
 neighbor 10.25.0.2 remote-as 2
 neighbor 10.25.0.2 route-map deny-all out
 neighbor 10.25.0.2 activate
 !
 neighbor 10.57.0.7 remote-as 7
 neighbor 10.57.0.7 route-map deny-all in
 neighbor 10.57.0.7 activate
!
!
